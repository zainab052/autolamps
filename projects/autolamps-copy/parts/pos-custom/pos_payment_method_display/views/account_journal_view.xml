<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="account_journal_view_form" model="ir.ui.view">
            <field name="name">Inherit : Account Journal POS Display Name Form</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="point_of_sale.view_account_journal_pos_user_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='journal_user']" position="after">
                    <field name="pos_journal_display_name"/> 
                </xpath>
            </field>
        </record>
    </data>
</openerp>
